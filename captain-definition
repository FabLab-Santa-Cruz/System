{
  "schemaVersion": 2,
  "dockerfileLines": [
    "FROM oven/bun:1",
    "WORKDIR /usr/src/app",
    "COPY prisma ./",
    "COPY ./package*.json ./",
    "ARG DATABASE_URL",
    "ARG NEXTAUTH_SECRET",
    "ARG NEXTAUTH_URL",
    "RUN bun pm untrusted",
    "RUN bun install",
    "COPY ./ ./",
    "RUN bun build",
    "EXPOSE 80",
    "CMD [ \"bun\", \"run\"  , \"start\"]"
  ]
}