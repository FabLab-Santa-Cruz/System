  "dockerfileLines": [
    "FROM oven/bun:latest",
    "WORKDIR /usr/src/app",

    "COPY ./ ./",
    "ARG DATABASE_URL",
    "ARG NEXTAUTH_SECRET",
    "ARG NEXTAUTH_URL",
    "RUN bun install",
    "RUN bun build",
    "EXPOSE 80",
    "CMD [ \"bun\", \"run\"  , \"start\"]"
